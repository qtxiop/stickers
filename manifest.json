{
	"background":
	{
		"scripts":
		[
			"background.js"
		]
	},
	"browser_action":
	{
		"default_icon":
		{
			"19":"19.png",
			"38":"38.png"
		},
		"default_popup":"popup.html"
	},
	"content_scripts":
	[
		{
			"css":
			[
				"content.css"
			],
			"js":
			[
				"content.js"
			],
			"matches":
			[
				"*://*/*"
			],
			"run_at":"document_end"
		}
	],
	"homepage_url":"http://qtxiop.github.io/stickers",
	"icons":
	{
		"16":"16.png",
		"48":"48.png",
		"128":"128.png"
	},
	"manifest_version":2,
	"name":"Stickers",
	"options_page":"options.html",
	"permissions":
	[
		"*://*/*",
		"contextMenus",
		"tabs",
		"unlimitedStorage"
	],
	"version":"1"
}